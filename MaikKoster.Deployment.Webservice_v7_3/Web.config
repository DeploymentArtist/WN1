    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="TraceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="D:\ViaMonstraWebServices\DeploymentWebservice\Logs\DeploymentWebserviceTrace.log"/>
      </listeners>
    </trace>
  </system.diagnostics>-->
</configuration>
