<!-- UnconditionalExclude for SYSTEM CONTEXT -->
	
<component context="System" type="Documents">
		
<displayName _locID="SYSTEM Exclusions">SYSTEM Exclusions</displayName>

		<role role="Data">
			
		<rules>
				
		<unconditionalExclude>
					
		<objectSet>
						
				<!-- I've moved the unconditional excludes for the WINDOWS dir to EXCLUDES due to wallpaper requirements  -->
		
 				<script>MigXmlHelper.GenerateDrivePatterns ("\Program Files (x86)\* [*] ", "Fixed")</script>        
				<script>MigXmlHelper.GenerateDrivePatterns ("\ProgramData\* [*] ", "Fixed")</script>        
				<script>MigXmlHelper.GenerateDrivePatterns ("\_SMSTaskSequence\* [*] ", "Fixed")</script>        
				<script>MigXmlHelper.GenerateDrivePatterns ("\MININT [*.*]", "Fixed")</script>
				<script>MigXmlHelper.GenerateDrivePatterns("* [ImageSuccessful.txt]", "Fixed")</script>
	
		</objectSet>
	
		</unconditionalExclude>
			
		</rules>
		
</role>
	
</component>

	
<!-- UnconditionalExclude for USER CONTEXT -->
	<component context="User" type="Documents">
		<displayName _locID="miguser.userdata">USER Exclusions</displayName>
		<role role="Data">
			<rules>
				<unconditionalExclude>
					<objectSet>
						<script>MigXmlHelper.GenerateDrivePatterns("%CSIDL_LOCAL_APPDATA%\* [*] ", "Fixed")</script>
					</objectSet>
				</unconditionalExclude>
			</rules>
		</role>
	</component>
	
	
<!-- Conditional exclude section -->		
		<component type="Documents" context="System">
		<displayName _locID="miguser.userdata">Conditional D Drive Exclusion</displayName>
		<role role="Data">
			<rules>
				<conditions>
					<condition>MigXmlHelper.DoesObjectExist("File","D:\User State Migration Tool")</condition>
				</conditions>
				<unconditionalExclude>
					<objectSet>
						<pattern type="File">D:\* [*]</pattern>
					</objectSet>
				</unconditionalExclude>
			</rules>
		</role>
	</component>
